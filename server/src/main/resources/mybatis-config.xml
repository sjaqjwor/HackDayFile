<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <package name="hackday.webtoon.mappers.ManuscriptMapper"/>
        <package name="hackday.webtoon.mappers.ThumbnailMapper"/>
        <package name="hackday.webtoon.mappers.ImageMapper"/>
        <package name="hackday.webtoon.mappers.UserMapper"/>
    </typeAliases>
    <mappers>
        <mapper resource="mappers/ManuscriptMapper.xml"/>
        <mapper resource="mappers/ImageMapper.xml"/>
        <mapper resource="mappers/ThumbnailMapper.xml"/>
        <mapper resource="mappers/UserMapper.xml"/>
    </mappers>
</configuration>

