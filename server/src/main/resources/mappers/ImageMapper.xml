<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="hackday.webtoon.mappers.ImageMapper">
    <insert id="addImage">
        INSERT INTO webtoon.image(user_id,manuscript_id,sequence,path,media_type,origin_name)
        VALUES (#{userId},#{manuscriptId},#{sequence},#{path},#{mediaType},#{name})
    </insert>
</mapper>